{
    "type": "postgres",
    "host": process.env.HOST,
    "port": process.env.PORT,
    "username": process.env.USER,
    "password": process.env.PASSWORD,
    "database": process.env.DB,
    "entities": [
        "src/app/models/*.ts"
    ],
    "migrations": [
        "src/database/migration/*.ts"
    ],
    "cli": {
        "migrationsDir": "src/database/migration"
    }
}
